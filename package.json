{
  "name": "@adobe/helix-embed",
  "version": "0.0.1",
  "description": "Example Package",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha",
    "test-ci": "nyc --reporter=text --reporter=lcov --check-coverage --branches 70 --statements 95 --lines 95 mocha --reporter xunit --reporter-options output=./junit/test-results.xml && codecov",
    "lint": "./node_modules/.bin/eslint .",
    "junit": "mocha --exit -R mocha-junit-reporter",
    "semantic-release": "semantic-release",
    "semantic-prepare": "node ./build/prepare.js",
    "preversion": "npm run junit",
    "deploy-iframely": "npm run zip && wsk action update embed embed.zip --kind nodejs:10 --web raw --web-secure false -p api http://iframe.ly/api/oembed -p api_key insert-here",
    "deploy-embedly": "npm run zip && wsk action update embed embed.zip --kind nodejs:10 --web raw --web-secure false -p api https://api.embedly.com/1/oembed -p key insert-here",
    "build": "wsk-builder -v",
    "deploy": "wsk-builder -v --deploy --test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-embed"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-embed/issues"
  },
  "homepage": "https://github.com/adobe/helix-embed#readme",
  "dependencies": {
    "request-promise-native": "^1.0.7",
    "request": "^2.88.0",
    "unfurl.js": "^3.0.1",
    "uri-js": "^4.2.2"
  },
  "devDependencies": {
    "@adobe/openwhisk-action-builder": "^1.1.0",
    "@pollyjs/adapter-fetch": "^2.4.0",
    "@pollyjs/adapter-node-http": "^2.4.0",
    "@pollyjs/core": "^2.4.0",
    "@pollyjs/persister-fs": "^2.1.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "^6.2.0",
    "@semantic-release/git": "^7.0.14",
    "@semantic-release/github": "^5.4.0",
    "@semantic-release/npm": "^5.1.13",
    "@semantic-release/release-notes-generator": "^7.2.1",
    "ajv": "^6.10.0",
    "codecov": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-header": "^2.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.12.0",
    "istanbul": "^1.1.0-alpha.1",
    "junit-report-builder": "^1.3.1",
    "mocha": "^6.1.1",
    "mocha-junit-reporter": "^1.21.0",
    "mocha-parallel-tests": "^2.0.3",
    "nyc": "^14.0.0",
    "semantic-release": "^15.13.18"
  },
  "snyk": true
}
