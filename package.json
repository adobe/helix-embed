{
  "name": "@adobe/helix-embed",
  "private": true,
  "version": "1.1.12",
  "description": "Helix Embed",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha -i -g Post-Deployment",
    "test-ci": "nyc --reporter=text --reporter=lcov --check-coverage --branches 70 --statements 95 --lines 95 mocha --reporter xunit --reporter-options output=./junit/test-results.xml -i -g Post-Deployment && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g Post-Deployment",
    "lint": "./node_modules/.bin/eslint .",
    "junit": "mocha --exit -R mocha-junit-reporter",
    "semantic-release": "semantic-release",
    "semantic-prepare": "node ./build/prepare.js",
    "preversion": "npm run junit",
    "deploy-iframely": "npm run zip && wsk action update embed embed.zip --kind nodejs:10 --web raw --web-secure false -p api http://iframe.ly/api/oembed -p api_key insert-here",
    "deploy-embedly": "npm run zip && wsk action update embed embed.zip --kind nodejs:10 --web raw --web-secure false -p api https://api.embedly.com/1/oembed -p key insert-here",
    "build": "wsk-builder -v --entryFile index.js",
    "deploy": "wsk-builder -v --raw-http --deploy --test=/_status_check/healthcheck.json --entryFile index.js -p UNSPLASH_AUTH=$UNSPLASH_AUTH",
    "deploy-sequences": "wsk-builder --no-build -no-hints -l latest -l major -l minor",
    "deploy-ci": "wsk-builder -v --deploy --test=/_status_check/healthcheck.json --entryFile index.js  -p UNSPLASH_AUTH=$UNSPLASH_AUTH --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci"
  },
  "wsk": {
    "name": "helix-services-private/embed@${version}",
    "linksPackage": "helix-services"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-embed"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-embed/issues"
  },
  "homepage": "https://github.com/adobe/helix-embed#readme",
  "dependencies": {
    "@adobe/helix-status": "7.0.1",
    "epsagon": "1.42.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.8",
    "unfurl.js": "5.0.1",
    "uri-js": "4.2.2"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.0",
    "@adobe/openwhisk-action-builder": "2.3.1",
    "@pollyjs/adapter-fetch": "2.6.3",
    "@pollyjs/adapter-node-http": "2.6.3",
    "@pollyjs/core": "2.6.3",
    "@pollyjs/persister-fs": "2.6.3",
    "@semantic-release/changelog": "3.0.5",
    "@semantic-release/commit-analyzer": "6.3.3",
    "@semantic-release/exec": "3.3.8",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/github": "5.5.5",
    "@semantic-release/npm": "5.3.4",
    "@semantic-release/release-notes-generator": "7.3.3",
    "ajv": "6.10.2",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "codecov": "3.6.1",
    "eslint": "6.6.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.18.2",
    "istanbul": "1.1.0-alpha.1",
    "junit-report-builder": "1.3.3",
    "mocha": "6.2.2",
    "mocha-junit-reporter": "1.23.1",
    "mocha-parallel-tests": "2.2.2",
    "nyc": "14.1.1",
    "semantic-release": "15.13.31"
  }
}
