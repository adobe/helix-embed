{
  "name": "@adobe/helix-embed",
  "private": true,
  "version": "1.2.24",
  "description": "Helix Embed",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc mocha -i -g Post-Deployment && npm run lint",
    "test-ci": "npm run lint && nyc --reporter=text --reporter=lcov --check-coverage --branches 70 --statements 95 --lines 95 mocha --reporter xunit --reporter-options output=./junit/test-results.xml -i -g Post-Deployment && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g Post-Deployment",
    "lint": "./node_modules/.bin/eslint .",
    "junit": "mocha --exit -R mocha-junit-reporter",
    "semantic-release": "semantic-release",
    "semantic-prepare": "node ./build/prepare.js",
    "preversion": "npm run test-ci",
    "deploy-iframely": "npm run zip && wsk action update embed embed.zip --kind nodejs:10 --web raw --web-secure false -p api http://iframe.ly/api/oembed -p api_key insert-here",
    "deploy-embedly": "npm run zip && wsk action update embed embed.zip --kind nodejs:10 --web raw --web-secure false -p api https://api.embedly.com/1/oembed -p key insert-here",
    "build": "wsk-builder -v",
    "deploy": "wsk-builder -v --deploy --test=/_status_check/healthcheck.json -p UNSPLASH_AUTH=$UNSPLASH_AUTH -p OEMBED_RESOLVER_KEY=$OEMBED_RESOLVER_KEY -p OEMBED_RESOLVER_PARAM=$OEMBED_RESOLVER_PARAM -p OEMBED_RESOLVER_URI=$OEMBED_RESOLVER_URI -p WHITELISTED_IPS=192.150.10.210",
    "deploy-sequences": "wsk-builder --no-build -no-hints -l latest -l major -l minor",
    "deploy-ci": "wsk-builder -v --deploy --test=/_status_check/healthcheck.json -p UNSPLASH_AUTH=$UNSPLASH_AUTH -p OEMBED_RESOLVER_KEY=$OEMBED_RESOLVER_KEY -p OEMBED_RESOLVER_PARAM=$OEMBED_RESOLVER_PARAM -p OEMBED_RESOLVER_URI=$OEMBED_RESOLVER_URI -p WHITELISTED_IPS=192.150.10.210 --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci"
  },
  "wsk": {
    "name": "helix-services/embed@${version}",
    "rawHttp": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-embed"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-embed/issues"
  },
  "homepage": "https://github.com/adobe/helix-embed#readme",
  "dependencies": {
    "@adobe/helix-fetch": "1.6.0",
    "@adobe/helix-epsagon": "1.2.0",
    "@adobe/helix-status": "7.1.3",
    "@adobe/openwhisk-action-logger": "2.2.0",
    "@adobe/openwhisk-action-utils": "4.2.2",
    "@braintree/sanitize-url": "4.0.1",
    "range_check": "1.4.0",
    "unfurl.js": "5.1.0",
    "uri-js": "4.2.2"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.0",
    "@adobe/helix-ops": "1.9.1",
    "@adobe/openwhisk-action-builder": "2.9.0",
    "@pollyjs/adapter-fetch": "4.0.4",
    "@pollyjs/adapter-node-http": "4.0.4",
    "@pollyjs/core": "4.0.4",
    "@pollyjs/persister-fs": "4.0.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "ajv": "6.12.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "codecov": "3.6.5",
    "eslint": "6.8.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.20.2",
    "junit-report-builder": "1.3.3",
    "mocha": "7.1.1",
    "mocha-junit-reporter": "1.23.3",
    "mocha-parallel-tests": "2.3.0",
    "nyc": "15.0.1",
    "proxyquire": "2.1.3",
    "semantic-release": "17.0.4"
  }
}
